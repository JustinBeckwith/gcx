{
  "name": "gcx",
  "version": "0.0.0",
  "description": "An API and CLI for deploying Google Cloud Functions in Node.js.",
  "bin": {
    "gcx": "./build/src/cli.js"
  },
  "main": "./build/src/index.js",
  "type": "module",
  "types": "./build/src/index.d.ts",
  "files": [
    "build/src",
    "src/.gcloudignore"
  ],
  "repository": "JustinBeckwith/gcx",
  "scripts": {
    "test": "c8 mocha build/test",
    "lint": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "watch": "tsc -p . --watch",
    "coverage": "c8 report --reporter=json"
  },
  "keywords": [
    "google",
    "cloud",
    "functions",
    "serverless"
  ],
  "engines": {
    "node": ">=12"
  },
  "author": "Justin Beckwith <justin.beckwith@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.0",
    "gaxios": "^5.0.0",
    "globby": "^13.1.1",
    "googleapis": "^105.0.0",
    "meow": "^10.1.2",
    "node-fetch": "^3.2.3",
    "ora": "^6.1.0",
    "update-notifier": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.1",
    "@types/sinon": "^10.0.11",
    "@types/update-notifier": "^5.1.0",
    "@types/uuid": "^8.3.4",
    "c8": "^7.11.0",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.4",
    "node-stream-zip": "^1.15.0",
    "semantic-release": "^19.0.2",
    "sinon": "^14.0.0",
    "typescript": "~4.7.0"
  },
  "c8": {
    "exclude": [
      "build/test"
    ]
  }
}
